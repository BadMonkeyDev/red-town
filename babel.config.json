{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    ["@babel/preset-react", {
     "runtime": "automatic"
    }]
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": [
          "uk",
          "en"
        ],
        "outputPath": "public/locales/{{locale}}/{{ns}}.json",
        "keyAsDefaultValue": false,
        "discardOldKeys": true
      }
    ]
  ]
}